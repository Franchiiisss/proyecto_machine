   "metadata": {},
   "outputs": [],
   "source": [
    "# Cargar datos procesados del notebook anterior\n",
    "user_features = pd.read_csv('data_processed/user_features.csv')\n",
    "movie_features = pd.read_csv('data_processed/movie_features.csv')\n",
    "data_final = pd.read_csv('data_processed/data_final.csv')\n",
    "\n",
    "print('='*60)\n",
    "print('DATOS CARGADOS')\n",
    "print('='*60)\n",
    "print(f'User features: {user_features.shape}')\n",
    "print(f'Movie features: {movie_features.shape}')\n",
